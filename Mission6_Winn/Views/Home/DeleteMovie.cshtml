@model Movie

<h2>Confirm Deletion</h2>
<h3>Are you sure you want to delete, @Model.Title </h3>
<form method="post">

    <input type="hidden" asp-for="MovieId"/>

    <button class="btn btn-outline-warning" type="submit">Delete</button>
    <a class="btn btn-info" asp-controller="Home" asp-action="MovieList">Canel</a>
</form>
    